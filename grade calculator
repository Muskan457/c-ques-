#include <iostream>
using namespace std;
int main()
{
    
    int a,b,c,d,e;
    cout << "enter ur five subject num" << endl;
    cin >> a >> b >> c >> d >> e ;
    int total=a+b+c+d+e;
    int avg=total/5;
    cout << "ur avg is : " << avg << endl;
    if(avg >=90 && avg<=100)
    {
        cout << "A" << endl;
    }
     else if(avg >=80 && avg<=89)
    {
        cout << "B" << endl;
    }
    else if(avg >=70 && avg<=79)
    {
        cout << "C" << endl;
    }
    else if(avg >=60 && avg<=69)
    {
        cout << "D" << endl;
    }
    else{
        cout << "F" << endl;
    }
    
    return 0;
    
    
}
